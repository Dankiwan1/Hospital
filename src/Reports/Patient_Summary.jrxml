	<queryString>
		<![CDATA[SELECT
     patient.`PATIENT_ID` AS patient_PATIENT_ID,
     patient.`TITLE` AS patient_TITLE,
     patient.`FIRST_NAME` AS patient_FIRST_NAME,
     patient.`LAST_NAME` AS patient_LAST_NAME,
     patient.`GENDER` AS patient_GENDER,
     patient.`EMAIL` AS patient_EMAIL,
     patient.`MOBILE_NO` AS patient_MOBILE_NO,
     patient_category.`MARITAL_STATUS` AS patient_category_MARITAL_STATUS,
     patient_category.`BLOOD_GROUP` AS patient_category_BLOOD_GROUP,
     patient_category.`TOWN` AS patient_category_TOWN,
     patient_category.`AGE` AS patient_category_AGE,
     patient_category.`VILLAGE` AS patient_category_VILLAGE
FROM
     `patient_category` patient_category INNER JOIN `patient` patient ON patient_category.`PATIENT_ID` = patient.`PATIENT_ID`
WHERE
      patient.`PATIENT_ID` = 1;]]>
	</queryString>
